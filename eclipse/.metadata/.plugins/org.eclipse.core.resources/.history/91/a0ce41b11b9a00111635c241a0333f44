package net.minecraft.src;

import java.io.File;
import java.io.FileInputStream;
import java.io.FileNotFoundException;
import java.io.FileOutputStream;
import java.io.IOException;
import java.util.Properties;

import net.minecraft.client.Minecraft;
import net.minecraft.src.powerc.PowerConfigurator;

public class mod_PowerConfigurator extends BaseMod {
	
	@Override
	public void modsLoaded()
	{
		if (ModLoader.isModLoaded("mod_WirelessRedstone"))
		{
			PowerConfigurator.initialize();
		}
	}
	
	public mod_PowerConfigurator()
	{
	}

	@Override
	public void load() {
	}
	
	@Override
	public String getVersion() {
		return "0.6";
	}
	
	static {
	}
}
