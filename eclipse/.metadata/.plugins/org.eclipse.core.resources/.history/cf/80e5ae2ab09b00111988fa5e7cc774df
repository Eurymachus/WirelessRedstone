package net.minecraft.src.wifiremote.network;

import java.io.DataInputStream;
import java.io.DataOutputStream;
import java.io.IOException;

import net.minecraft.src.powerc.network.PacketPowerConfig;
import net.minecraft.src.wifi.TileEntityRedstoneWireless;
import net.minecraft.src.wifi.network.PacketIds;
import net.minecraft.src.wifi.network.PacketPayload;
import net.minecraft.src.wifi.network.PacketUpdate;

public class PacketWirelessRemoteGui extends PacketPowerConfig 
{
	public PacketWirelessRemoteGui()
	{
		super(PacketIds.WIFI_POWERCGUI);
	}
	
	public PacketWirelessRemoteGui(int x, int y, int z) {
		this();
		xPosition = x;
		yPosition = y;
		zPosition = z;
	}
}
