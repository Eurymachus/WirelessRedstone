package net.minecraft.src.powerc.network;

import java.io.DataInputStream;
import java.io.DataOutputStream;
import java.io.IOException;

import net.minecraft.src.wifi.TileEntityRedstoneWireless;
import net.minecraft.src.wifi.network.PacketIds;
import net.minecraft.src.wifi.network.PacketPayload;
import net.minecraft.src.wifi.network.PacketUpdate;

public class PacketPowerConfigGui extends PacketUpdate 
{
	public PacketPowerConfigGui() {
		super(PacketIds.WIFI_POWERCGUI);
	}
	
	public PacketPowerConfigGui(TileEntityRedstoneWireless entity) {
		this();
		xPosition = entity.getBlockCoord(0);
		yPosition = entity.getBlockCoord(1);
		zPosition = entity.getBlockCoord(2);
		PacketPayload p = new PacketPayload(1,1,2);
		p.stringPayload[0] = entity.currentFreq;
		p.stringPayload[1] = entity.getFreq();
			p.intPayload[0] = 0;
		p.floatPayload[0] = 0;
		this.payload = p;
	}
}
